	<Methods>
			<Method>
			
			   <Name>getSpeechInputModes</Name>
			   <ID>1200</ID>
			         <Type>Request</Type>
			  		   <Description>Get the available modes for speech input, i.e. language and mode (oneshot,fullword,spelling).</Description>
			<Response>getSpeechInputModes</Response>
			  		   <Parameters>
			    		        <Parameter>
			    		            <Name>getSpeechInputModes_R_categoryId</Name>
			    		            <Description>ALL_CATEGORIES is the root categoryId.</Description>
			    		          <ID>7</ID>
			    		<Type>org_harman_nav_ctrl_di_POIServiceTypes::CategoryID</Type>
			    		           <IsDefault>false</IsDefault>
			    		        </Parameter>
			    		        <Parameter>
			    		            <Name>getSpeechInputModes_R_languageCode</Name>
			    		            <Description>Language for which the speech input should be performed.</Description>
			    		          <ID>8</ID>
			    		<Type>org_harman_nav_ctrl_di_SpeechLocationInputTypes::Locales</Type>
			    		           <IsDefault>false</IsDefault>
			    		        </Parameter>
			  		   </Parameters>
			</Method>
			<Method>
				<Name>getSpeechInputModes</Name>
