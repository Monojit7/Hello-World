package com.harman.vehicle

import model "VehicleDataTypes.fidl"

interface TimeDateProvider
{
	version { major 0 minor 1}
	
	/*This signal provides the time and date information. */
	attribute TimeDate timeDate readonly
	
	struct TimeDate extends com.harman.vehicle.VehicleDataTypes.VehiclePropertyBase
	{
		// 24 hr format
		UInt8 day
		UInt8 month
		UInt16 year
		UInt8 hour
		UInt8 minute
		UInt8 second
	}
}